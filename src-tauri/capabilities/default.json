{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "updater:default",
    "process:default",
    "dialog:default",
    "core:window:allow-set-title",
    "deep-link:default",
    "oauth:allow-start",
    "oauth:allow-cancel",
    "shell:default",
    "shell:allow-open"
  ]
}